{
  "name": "omni-logistics-frontend",
  "version": "1.0.0",
  "description": "Modern fleet management and vehicle tracking platform frontend",
  "main": "public/index.html",
  "scripts": {
    "start": "npx http-server public -p 8080",
    "dev": "npx live-server public --port=8080",
    "dev:functions": "netlify dev",
    "build": "echo 'Static site - no build needed'",
    "deploy": "netlify deploy --prod --dir=public",
    "deploy:functions": "netlify deploy --prod"
  },
  "keywords": [
    "fleet-management",
    "vehicle-tracking",
    "logistics",
    "gps-tracking",
    "omni-logistics"
  ],
  "author": "Omni Logistics",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/omni-logistics-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/omni-logistics-frontend/issues"
  },
  "homepage": "https://omni-logistics.netlify.app",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "nodemailer": "^6.10.1"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "live-server": "^1.2.2",
    "netlify-cli": "^17.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
